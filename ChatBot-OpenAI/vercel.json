{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@now/python"
    },
    {
      "src": "templates/**/*.html",
      "use": "@now/templates"
    },
    {
      "src": "static/images/**/*",
      "use": "@now/static"
    },
    {
      "src": "static/styles.css",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ]
}
