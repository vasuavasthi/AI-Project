{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@now/python"
    },
    {
      "src": "static/**/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/templates/(.*)",
      "dest": "/templates/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ]
}

